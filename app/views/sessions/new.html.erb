<body style="text-align: center; background-color: #F0B27A">
<h1>Login</h1>
<%= form_with method: "post", url: "/sessions" do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Submit" %>
  </div>
<% end%>